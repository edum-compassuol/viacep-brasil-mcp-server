{
  "name": "@edum-compassuol/viacep-brasil-mcp-server",
  "version": "0.1.0",
  "description": "MCP Server para consulta de endereços brasileiros através da API ViaCEP",
  "private": false,
  "type": "module",
  "bin": {
    "viacep-brasil-mcp-server": "./build/index.js"
  },
